{
  "expo": {
    "name": "MyTrabzon",
    "slug": "mytrabzon-",
    "version": "1.0.57",
    "orientation": "portrait",
    "scheme": "mytrabzon",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.litxtech.mytrabzon",
      "versionCode": 56,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.litxtech.mytrabzon",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "mytrabzon",
              "host": "*"
            },
            {
              "scheme": "litxtech",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.litxtech.mytrabzon",
      "buildNumber": "56",
      "icon": "./assets/images/icon.png",
      "appStoreUrl": "https://apps.apple.com/app/mytrabzon/id1234567890",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "MyTrabzon needs access to your photo library to let you share photos and videos with your friends and create posts.",
        "NSPhotoLibraryAddUsageDescription": "MyTrabzon needs permission to save photos and videos to your photo library.",
        "NSCameraUsageDescription": "MyTrabzon needs access to your camera to let you take photos and videos to share with your friends and create posts.",
        "NSMicrophoneUsageDescription": "MyTrabzon needs access to your microphone for voice and video calls, and to record videos with audio.",
        "NSLocationWhenInUseUsageDescription": "MyTrabzon uses your location to show nearby users, events, matches, and help you discover local activities in Trabzon. Location is only used when the app is active and you have enabled location sharing in settings.",
        "NSLocationAlwaysUsageDescription": "MyTrabzon does not use location in the background. This permission is not required.",
        "NSUserTrackingUsageDescription": "MyTrabzon does not track users across apps or websites. This permission is not used.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "mytrabzon",
              "litxtech"
            ]
          }
        ]
      },
      "associatedDomains": [
        "applinks:mytrabzon.app",
        "applinks:www.litxtech.com"
      ]
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      [
        "expo-apple-authentication",
        {
          "appleTeamId": "9W6CR7KXM7"
        }
      ],
      [
        "expo-dev-client",
        {
          "android": {
            "package": "com.litxtech.mytrabzon"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "minSdkVersion": 24,
            "javaVersion": "17",
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so",
                "**/libjsc.so"
              ]
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "368ac56e-da84-480e-b163-f04077df7fa6"
      },
      "agoraAppId": "d1e34b20cd2b4da69418f360039d254d",
      "agoraCertificate": "d0c65c85891f40c680764c5cf0523433",
      "cli": {
        "appVersionSource": "local"
      },
      "privacy": "unlisted",
      "description": "Trabzon'un dijital sesi. Sosyal medya, etkinlikler, halı saha maçları ve daha fazlası.",
      "primaryColor": "#DC2626",
      "privacyPolicy": "https://www.litxtech.com/privacy-policy",
      "termsOfService": "https://www.litxtech.com/terms-of-service",
      "supportUrl": "https://www.litxtech.com/support",
      "childSafetyPolicy": "https://www.litxtech.com/child-safety-policy",
      "communityPolicy": "https://www.litxtech.com/community-policy",
      "owner": "mytrabzon"
    },
    "sdkVersion": "54.0.0",
    "jsEngine": "hermes",
    "androidNavigationBar": {
      "visible": "immersive",
      "backgroundColor": "#ffffff"
    },
    "icon": "./assets/images/icon.png"
  }
}
